{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "desktop-web-app",
	"version": "0.1.0",
	"identifier": "com.desktop-web-app.app",
	"build": {
		"beforeDevCommand": "npm run dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "npm run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": ["icons/icon.ico"]
	}
}
